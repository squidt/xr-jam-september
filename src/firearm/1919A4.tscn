[sub_resource type="GDScript" id="GDScript_tjfkl"]
resource_name = "Globus"
script/source = "@tool 
extends MeshInstance3D

@export var freq: float = 1.0
@export var amp: float = 1.0
var _time: float = 0.0


func _process(delta: float) -> void:
	if visible and !Engine.is_editor_hint():
		_time += delta

		material_override = material_override as StandardMaterial3D
		var sine = sin(_time * TAU / freq) * amp
		var v = remap(sine, -1.0, 1.0, 0.0, .01)
		material_override.albedo_color.a = v
"

